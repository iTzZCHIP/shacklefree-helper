{
    "manifest_version": 3,
    "name": "shacklefree.io Helper",
    "version": "0.1",
    "description": "Ã–ffnet die aktuelle Seite auf shacklefree.io",
    "author": "Paul Zischler",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.shacklefree.in/*"
    ],
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}